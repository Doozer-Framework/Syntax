{
  "name": "doozer-framework/syntax",
  "license": "MIT",
  "description": "Syntax trait enriching classes with parser, interpreter and compiler for expression support.",
  "type": "library",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "keywords": [
    "syntax",
    "expression",
    "language",
    "doozer-framework",
    "clickalicious"
  ],
  "homepage": "https://doozer.readme.io/",
  "support": {
    "issues": "https://github.com/Doozer-Framework/Syntax/issues",
    "wiki": "https://github.com/Doozer-Framework/Syntax/wiki",
    "tests": "https://doozer-framework.github.io/Syntax"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "doozer-framework/exception": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "fzaninotto/faker": "^1.3",
    "symfony/var-dumper": "^3.1",
    "codeclimate/php-test-reporter": "dev-master as 1.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Doozer\\Syntax\\": "src/Syntax/"
    },
    "exclude-from-classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Doozer\\Syntax\\Tests\\": "tests/Syntax/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "use-include-path": false,
    "preferred-install": "dist"
  }
}
